{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true,
            "modules": true
        }
    },
    "globals": {
        "sendMessages": true,
        "import": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "no-mixed-spaces-and-tabs": [0],
        "no-useless-escape": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-console": "off",
        "no-unexpected-multiline":"off"
    }
}